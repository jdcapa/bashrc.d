#!/usr/bin/env bash

if [ -f `which powerline-daemon` ]; then
    if [ -f ${PYTHON_LOC}/lib/python3.5/site-packages/powerline/bindings/bash/powerline.sh ]; then
        powerline-daemon -q
        POWERLINE_BASH_CONTINUATION=1
        POWERLINE_BASH_SELECT=1
        . ${PYTHON_LOC}/lib/python3.5/site-packages/powerline/bindings/bash/powerline.sh
    fi
fi
