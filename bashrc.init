
# This churns through files in $HOME/.bashrc.d if they are executable.
if [ -d $HOME/.bashrc.d ]; then
    for x in $HOME/.bashrc.d/* ; do
        if [[ "${x##*/}" != "bashrc.init" ]]; then
            test -f "$x" || continue
            test -x "$x" || continue
            . "$x"
        fi
    done
fi
